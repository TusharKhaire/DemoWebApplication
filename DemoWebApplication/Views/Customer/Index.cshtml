@model IEnumerable<DemoWebApplication.AccountMaster>
    <h4>All Customers</h4>

    <p>
        @Html.ActionLink("Create New", "addCustomer")
    </p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AccountName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MobileNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email )
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateofBirth )
        </th>s
    </tr>
@foreach(var item in Model)
{
    <tr>
        <th>
            @Html.DisplayFor(model => item.AccountName)
        </th>
        <th>
            @Html.DisplayFor(model => item.Address)
        </th>
        <th>
            @Html.DisplayFor(model => item.MobileNo)
        </th>
        <th>
            @Html.DisplayFor(model => item.Email)
        </th>
        <th>
            @Html.DisplayFor(model => item.DateofBirth)
        </th>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.AccountCode }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.AccountCode })
        </td>
    </tr>
}

</table>


